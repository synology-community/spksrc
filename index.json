---
layout: null
---
{
  "packages": [
{% for package in site.data.packages %}
  {
    "package": "{{ package.package }}",
    "desc": "{{ package.description }}",
    "version": "{{ package.version }}",
    "maintainer": "{{ package.maintainer }}",
    "maintainer_url": "{{ package.maintainer_url }}",
    "link": "{{ package.link }}"
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
  ]
}